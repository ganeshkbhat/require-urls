{
  "name": "require-urls",
  "version": "0.0.7",
  "description": "Deno like and GoLang like Mode in ES and commonjs: Replace nodejs require function with requireurls function using repository",
  "main": "index.js",
  "scripts": {
    "start": "node --experimental-loader=./loader.mjs",
    "demojs": "node ./demos/demo.js",
    "demomjs": "node ./demos/demo.mjs",
    "test": "mocha --reporter spec --recursive --experimental-loader=./loader.mjs",
    "testconfig": "mocha --reporter spec --recursive --config ./mocha.test.config.js --experimental-loader=./loader.mjs"
  },
  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganeshkbhat/requireurl.git"
  },
  "author": "Ganesh Bhat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ganeshkbhat/requireurl/issues"
  },
  "homepage": "https://github.com/ganeshkbhat/requireurl#readme",
  "devDependencies": {
    "@types/node": "^18.11.3",
    "chai": "^4.3.6",
    "hpp": "^0.2.3",
    "mocha": "^10.0.0",
    "nsp": "^3.2.1",
    "nsp-api": "^1.0.3",
    "safe-regex": "^2.1.1",
    "sinon": "^14.0.0",
    "unimported": "^1.22.0"
  },
  "keywords": [
    "require",
    "urls",
    "remote",
    "requireurl",
    "require-url",
    "requireurls",
    "require-urls",
    "urlrequire",
    "url-requires",
    "url-require",
    "urlrequires",
    "deno style require",
    "golang style require",
    "cache remote require files"
  ],
  "dependencies": {
    "concurrency.js": "^0.0.1",
    "filelock.json": "^0.0.2",
    "fssys": "^1.0.1",
    "get-imported": "^0.0.5",
    "git-apis": "^0.0.2",
    "hasher-apis": "^0.0.1",
    "mercurial-apis": "^0.0.1",
    "root-dirs": "^0.0.2",
    "svn-apis": "^0.0.1"
  }
}
